/*登录*/
.login_nav{width:355px;height:46px;overflow: hidden;float:left}
.login_navw .nav_a{font-size:16px;color:#ff8a10;float:right;margin-top:16px;border:1px solid #ff8a10;padding:5px 15px;border-radius: 8px;}
.login_navw .nav_a:hover{color:#fff;background: #ff8a10}
.login_navw{width:1160px;height:67px;margin:0 auto;padding-top:15px;}
.login_warp{background: url(/img/rbg.jpg) center center; }
.login_div{overflow:auto;width: 1160px;margin: auto;height:595px;}

.login_div .login-form {width:334px;text-align: center;background: rgba(255,255,255,0.8);padding:25px 33px 20px 33px; ;
    float: right;margin-top:90px;filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#8fffffff,endColorstr=#8fffffff);}
.login_div  .form-control,.register  .form-control,.f_input .form-control{height: 45px;line-height: 45px;}
.login_div .login-form h2{margin-bottom: 20px;font-size:24px;}
.login_div .login-form .p_f{float:right;color:#fd9707;margin-bottom:5px;}
.login_div .login-form .p_f a{color:#fd9707}
.login_footer{height:143px;text-align:center;margin-top:38px;}
.login_footer .p1{margin-bottom:27px;color:#4c4c4c}
.login_footer .p1 img{margin-right:8px}
.login_footer .p2{margin-bottom:17px}
.login_footer .p3 a{margin-right:20px;color:#4c4c4c;}
.login_footer .p3 span{margin-right:20px;color:#4c4c4c;}
.login_footer .p3 {margin-bottom:20px;}
.login_footer .p3 a:hover{text-decoration:underline}
.form-horizontal .form-group{position: relative;}
.form-horizontal .form-group span{display: inline-block;height: 45px;width: 45px;position: absolute;background: url(/img/l2.png);left: 0;top: 1px;left: 1px;border-right: 1px solid #ededed}
.form-horizontal .form-group .u1{background-position: -12px -8px;}
.form-horizontal .form-group .u2{background-position: -210px -8px}
.form-horizontal .form-group .u3{background-position: -138px -8px}
.form-horizontal .form-group .u4{background-position: -74px -8px}
.form-horizontal .form-group .u5{background: url(/img/u3_mo.png)}

.form-horizontal .btn-logoin{margin:5px 0 8px 0;width: 100%;padding: 0;height: 45px;line-height: 45px;background: #ff8a10;color: #fff;font-size: 16px;}
.login_a {color: #999;margin-top: 8px;position: relative;overflow:auto;}
.login_a a{color:#f49500;float:right;}
.login_a span{float:left;}
.login_a a:hover{
    text-decoration: underline;;
}

.yanzh_inp input{float: left;text-indent: 16px;width: 210px;background: #fbfbfb}
.yanzh_inp {overflow: hidden;}
.yanzh_inp i{display: inline-block;float: right;width: 95px;height: 45px;line-height: 45px;color: #ddd;border:1px solid #ddd;border-radius: 5px;text-align:center}
.yanzh_inp .imgcode{border:none;float:none}

/*注册协议*/
.alertbox {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 100;
    color: #5e5e5e;background: rgba(0,0,0,0.3);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#3f000000);    /*IE8支持*/
    display: none;
}.alertbox .reg_alertbox {
     position: absolute;
     background: #fff;
     top: 150px;
     left: 50%;
     margin-left: -255px;
     width: 510px;
     border-radius: 8px;padding-bottom:25px;
 }
.alertbox  .content{
    padding: 0 25px 0px 20px;
}
.alertbox  .title{
    height:40px;background: #fe8749;line-height: 40px;color:#fff;
    font-weight:bold;padding:0 20px;margin-bottom:25px;border-radius: 8px 8px 0 0;
}
.xyTextAlert{height:400px;
    overflow:auto;
}
.xyTextAlert p{margin-bottom:10px;    padding-right: 5px;}
.alertbox  .title img{vertical-align: middle;cursor: pointer;float:right;margin-top:5px;}