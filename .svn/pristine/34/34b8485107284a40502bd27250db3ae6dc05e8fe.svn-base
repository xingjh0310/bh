/* 表格*/
.table{border:1px solid #ededed}
.table td{text-align:center;padding: 15px 8px;line-height: 1.428571429; border-top: 1px solid #ddd;border-right: 1px solid #ddd;}
.table > thead > tr > th {vertical-align: bottom;padding:8px;border-right: 1px solid #ddd;}
.table td:last-child,.table > thead > tr > th:last-child{border-width:1px 0 1px 0 }

/*  按钮 */
.btn{ display: inline-block; padding: 6px 12px; margin-bottom: 0; font-size: 14px; font-weight: normal; line-height: 1.428571429; text-align: center; white-space: nowrap; vertical-align: middle; cursor: pointer; background-image: none;  border-radius: 4px;   }
.btn-default{color: #333;  background-color: #fff; border-color: #ccc; }
.btn-default:hover{background: #ebebeb}
.btn-shaw{width: 100px;height: 32px;background: #ffb700;display: inline-block;margin: 0 auto;border-radius: 6px; }
.btn-shaw span{display: block;width: 100%;height: 28px;background: #ffde00;border-radius: 6px;text-align: center;line-height: 28px;color: #7c4121;font-size: 14px;  }
.btn-info {color: #fff;background-color: #428bca;border-color: #357ebd; }
.btn-red {color: #fff;background-color: #d9534f;border-color: #d43f3a; }
.btn-gre {color: #fff;background-color: #5cb85c;border-color: #4cae4c; }
/*分页 */
.pagination {display: inline-block;padding-left: 0;margin: 20px 0;border-radius: 4px;}
.pagination>li {display: inline;}
.pagination>li>a, .pagination>li>span {position: relative;float: left;padding: 6px 12px;margin-left: -1px;line-height: 1.428571429;text-decoration: none;background-color: #fff;border: 1px solid #ddd; }
.pagination>li>a:hover{background: #ebebeb  }
.pagination>li .active{background: #ebebeb }
.pagination_1{overflow: auto; background:#fff;display: table;margin: 0 auto}
.pagination_1 li{float: left;border:1px solid #dddddd;}
.pagination_1 li:hover{border:1px solid #fe8749}
.pagination_1 li:last-child{margin:0;}
/*表单*/
.form-control {display: block;width: 100%;font-size: 14px;line-height: 1.428571429;color: #555;vertical-align: middle;background-color: #fff;background-image: none;border: 1px solid #ededed;margin-bottom: 20px;text-indent: 60px; }

.form-line{overflow: auto; }
.form-line label{float: left;display: table;line-height: 34px;  }
.form-line .form-control{ width: 60%;}


/*单选按钮组 */
.regular-radio:checked + label {background-color: #e9ecee;color: #99a1a7;border: 1px solid #51a3f5;margin-right:2px; }
.regular-radio + label {margin-right:2px;-webkit-appearance: none;background-color: #fafafa;border: 1px solid #cacece;padding:6px;border-radius: 50px;display: inline-block;position: relative; }
.regular-radio:checked + label:after {content: ' ';width: 6px;height: 6px;border-radius: 50%;position: absolute;top: 3px;background: #51a3f5;box-shadow: inset 0px 0px 10px rgba(0,0,0,0.3);text-shadow: 0px;left: 3px;font-size: 32px;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#3f000000,endColorstr=#3f000000);}
.regular-radio {display: none; }


/*复选框*/
.regular-checkbox + label {vertical-align: middle;border: 1px solid #fe8749;padding:6px;display: inline-block;position: relative;margin-right: 2px; }
.regular-checkbox {display: none; }
.regular-checkbox:checked + label:after {text-align: center;border:1px solid #fe8749;line-height: 12px;content: '\2714';font-size: 12px;position: absolute;top: -1px;left: -1px;color: #fe8749;height: 12px;width: 12px; }
.check-lab{    display: inline-block; }
.regular-checkbox1{vertical-align: middle;margin-right: 5px;}

/*列表组*/
.list-group {padding-left: 0;margin-bottom: 20px; }
.list-group-item {position: relative;display: block;padding: 10px 15px;margin-bottom: -1px;background-color: #fff;border: 1px solid #ddd; }
.badge {display: inline-block;min-width: 10px;padding: 3px 7px;font-size: 12px;font-weight: bold;line-height: 1;color: #fff;text-align: center;white-space: nowrap;vertical-align: baseline;background-color: #999;border-radius: 10px; }
.list-group-item>.badge {float: right; }

/*缩略图*/
.thumbnail {display: block;background-color: #fff;border-radius: 4px;-webkit-transition: all .2s ease-in-out;transition: all .2s ease-in-out;width: 350px; }
.thumbnail .caption {padding: 9px 0;color: #333; }
.thumbnail .caption .p1:hover a{color:#ff8a10}
.thumbnail .caption .p2 {
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;font-size: 16px;
    -webkit-box-orient: vertical;
}
.thumbnail .caption .p1 {
    min-height: 20px;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;margin-bottom:20px;
    display: -webkit-box;
    -webkit-line-clamp: 2;font-size: 16px;
    -webkit-box-orient: vertical;
}
.thumbnail .caption p:nth-child(2) {
     color: #999;
 }.thumbnail img {
       width: 100%;
       height: 122px;
   }
.thumbnail .caption .p2{text-align:right;color:#999;}
.thumbnail .caption .p1{font-size: 16px}
.thumbnail .caption .p2{font-size: 12px;}
/*三列列表*/
.list-line{width: 710px;overflow: auto;zoom:1; }
.list-line>li{width: 33.3%;float: left; }
.list-col{overflow: auto;margin-top: 8px; }
.list-col li{float: left;text-align: center; }
.list-col li a{display: inline-block;width: 80px;text-align: center; }
.list-img>div{display: inline-block; }
.list-row >p{padding-left: 8px;color: #888 }

