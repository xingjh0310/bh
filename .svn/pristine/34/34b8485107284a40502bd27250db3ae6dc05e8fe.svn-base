/*弹出框*/
.modal-alert{
  position: fixed;height:100%;width:100%;background: rgba(0,0,0,0.3);top:0;left:0;z-index: 9999;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#3f000000);    /*IE8支持*/
}
.modal-dialog-alert{width:480px;background: #fff;margin:180px auto 0;text-align:center;padding-bottom: 40px;}
.modal-conten-alert img{margin-top:40px;}
.modal-conten-alert .p1{color:#ff8a10;margin:35px 0 35px 0;font-size:16px;}
.modal-conten-alert .btn_zd{font-size:16px;color:#ff8a10;border:1px solid #ff8a10;padding:3px 30px;display:table;margin:0 auto;border-radius: 8px;}

.qq_btn i{    width: 100px;
    height: 28px;
    display: inline-block;
    border: 1px solid #4E5052;margin-right:10px;
    text-align: center;cursor: pointer;
    line-height: 26px}
.qq_btn{position: relative;}
.qq_btn>div{    left: -80px;
    position: absolute;width:230px;background:url("/img/qqimg1.png") no-repeat;padding:10px 15px;top:-114px;display:none;    height: 90px;
}
.qq_btn>div .p1{color:#333;font-weight: bold}
.qq_btn>div .p2{color:#999}
.qq_btn>div.qq1{
    height: 50px;background-size: 100% 100%;top:-70px;
}
.qq_btn:hover div{display: block;}


