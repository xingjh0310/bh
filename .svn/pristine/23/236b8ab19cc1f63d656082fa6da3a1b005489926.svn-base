/*home/index*/
/*---banner---*/
.banner {width:100%;height:360px;margin:0 auto;position: relative;overflow: hidden}
.banner_main{width:100%;height:360px;position: absolute;left:0;top:0;}
.banner_main li{display:block;height:360px;position: absolute;left:0;top:0;background:#ebebeb no-repeat center center;background-size:100%;width: 100%;}
.banner_main li a{display:block;height:360px;}
.banner_span {height:35px;position: absolute;left:0;bottom:0;zoom:1;left: 50%;margin-left: -70px;}
.banner_span span {width: 10px;height: 10px;display:block;float:left;margin-left:10px;cursor: pointer;border-radius: 50%;border:1px solid #fff;}
.banner_span p {height:35px;margin:0 auto;}
.banner_span .banner_active{background: #f1ad19;border:1px solid #f1ad19}
.banner_fix_main{width: 1103px;margin: 0 auto;position: relative;}
.banner_fix{display:none;color:#fff;text-align:center;height: 360px;width: 352px;background: rgba(0,0,0,0.4);position: absolute;z-index: 1;right: 0;filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#2f000000,endColorstr=#2f000000);    /*IE8支持*/}
.banner_fix h2{font-size: 24px;margin-bottom:25px;}
.banner_fix h1{font-size: 58px;}
.banner_fix h1 span{font-size: 16px;}
.banner_fix .msg{margin:55px 0 40px 0}
.banner_fix .msg1 h2{font-size:24px;margin:80px 0 45px 0;}
.banner_fix .msg1 h3{font-size:30px}
.banner_fix .msg1 h3 span{font-size:45px;}
.banner_fix .btn{padding:0px;margin-bottom: 10px;width: 230px;height: 45px;font-size:20px;background: #ff8200;color: #fff;line-height: 45px;border-radius: 40px;}
.banner_fix .btn:hover{box-shadow: 2px 2px 10px #f47d01 inset;}
.banner_fix .p1 a{color: #ff8200;position:relative}
.banner_fix .p1 a:before{  content: "";  position: absolute;  left: 50%;  bottom: -2px;  width: 0;  height: 1px;  background: #ff8200;  transition: all .3s;  }
.banner_fix .p1 a:hover{  text-decoration: underline }

