/*advantage*/
.advantage{ width: 1103px; height:125px;margin:0 auto 0 ;overflow:auto;background: #fff; -webkit-box-shadow: 0px 2px 2px #ccc;
    -moz-box-shadow: 0px 2px 2px #ccc;
    box-shadow: 0px 2px 2px #ccc;   }
.advantage .advantageul{ width: 100%; height: 100%;}
.advantage .advantageul li{ float: left; width: 33.3%; height: 100%;transform-style: preserve-3d; }

.advantage .advantageul li img{ display: block; float: left;  margin:20px 35px 0  55px;
    -webkit-animation:rotate1 linear 0.5s  0s;
    -moz-animation:rotate1 linear 0.5s  0s;
    -ms-animation:rotate1 linear 0.5s  0s;
    animation:rotate1 linear 0.5s  0s; cursor: pointer }
.advantage .advantageul li img:hover{
    -webkit-animation:rotate linear 0.5s  0s;
    -moz-animation:rotate linear 0.5s  0s;
    -ms-animation:rotate linear 0.5s  0s;
    animation:rotate linear 0.5s  0s;
}
/*动画定义*/
@-webkit-keyframes rotate{
    0%{
        -webkit-transform:rotateY(0);
    }
    100%{
        -webkit-transform:rotateY(360deg);
    }
}
@-moz-keyframes rotate{
    0%{
        -moz-transform:rotateY(0);
    }
    100%{
        -moz-transform:rotateY(360deg);
    }
}
@-ms-keyframes rotate{
    0%{
        -moz-transform:rotateY(0);
    }
    100%{
        -moz-transform:rotateY(360deg);
    }
}
@keyframes rotate{
    0%{
        transform:rotateY(0);
    }
    100%{
        transform:rotateY(360deg);
    }
}
@-webkit-keyframes rotate1{
    0%{
        -webkit-transform:rotateY(0);
    }
    100%{
        -webkit-transform:rotateY(-360deg);
    }
}
@-moz-keyframes rotate1{
    0%{
        -moz-transform:rotateY(0);
    }
    100%{
        -moz-transform:rotateY(-360deg);
    }
}
@-ms-keyframes rotate1{
    0%{
        -ms-transform:rotateY(0);
    }
    100%{
        -ms-transform:rotateY(-360deg);
    }
}
@keyframes rotate1{
    0%{
        transform:rotateY(0);
    }
    100%{
        transform:rotateY(-360deg);
    }
}
