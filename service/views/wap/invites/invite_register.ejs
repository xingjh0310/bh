<script>

    // 获取参数
    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }
    var mobile =  getQueryString('mobile');
    var invite = getQueryString('invite');

    $('#xi').click(function () {
        $('.alertbox').show();
        $("body").css({'overflow':'hidden','position':'fixed'})
    })
    //fastclick-----------------
    $(function() {
        FastClick.attach(document.body);
    });
    var flag = true;
    var mobileReg =/^1[3|4|5|7|8|9][0-9]\d{8}$/;//手机号正则
    var pwdReg = /^(?!\$)\S{6,20}$/;//密码正则
    var code = /\w+/;//验证码正则
    layui.use('layer', function(){
        var layer = layui.layer;
        var all  =  {
            //自定义复选框
            inner_check:function (){
                var check =  $("#check");
                var inner = $("#inner_check");
                check.on('click',function () {
                    flag = !flag
                    if (flag){
                        inner.show();
                    }else {
                        inner.hide();
                    }
                })
            },
            //下载app
            download:function () {
                    var url = {
                        android:'http://shouji.360tpcdn.com/180131/b860ccaecf053ce6f721710bfad66f2b/com.BoHengJinFu.bohomm_18.apk',
                        ios:'https://itunes.apple.com/cn/app/%E9%93%82%E6%81%92%E9%87%91%E6%9C%8D-%E5%AE%89%E5%85%A8%E9%9D%A0%E8%B0%B1%E7%9A%84%E6%8A%95%E8%B5%84%E7%90%86%E8%B4%A2%E5%B9%B3%E5%8F%B0/id1181258117?mt=8'
                    }
                    if(browser.versions.wx){
                        layer.msg('请转至手机浏览器下载',{time:3000});
                        return
                    }else{
                        if ((browser.versions.ios &&  browser.versions.mobile)) {
                            window.location = "BoHengJinFu://";
                            window.setTimeout(function() {
                                window.location = url.ios;//ios下载地址
                            },500);

                        }else if ((browser.versions.android &&  browser.versions.mobile)) {
                            try {
                                window.location = "launchapp://android/launch";
                                setTimeout(function(){
                                    window.location=url.android; //android下载地址
                                },500);
                            } catch(e) {}
                        }
                    }

            },
            registerCode: function () {
                $('#time').on('click',function () {
                    var mobile = $('#mobile').val();
                    if(mobileReg.test(mobile) === true){
                        $.post('/user/sendRegisterCode',{mobile:mobile}, function (res) {

                            if(res.err == null){//如果请求成功
                                all.countdown();//短信倒计时方法
                            }else{
                                layer.msg(res.err,{time:3000});
                                return
                            }
                        });
                    }else{
                        layer.msg('请输入正确的手机号',{time:3000});
                        return
                    }

                });
            },
            //倒计时
            countdown:function () {
                //短信验证码
                var count = 120;
                if(mobileReg.test($('#mobile').val()) === false){
                    layer.msg('请输入正确的手机号',{time:3000});
                    return
                }else{
                    $('.register_tips').hide();
                    var t = setInterval(function () {
                        count --;
                        $('#time').val(count+'s');
                        $('#time').attr('disabled',true)
                        if (count==0){
                            $('#time').val("获取验证码");
                            $('#time').attr('disabled',false);
                            clearInterval(t);
                        }
                    },1000)
                }
            },
            //点击注册
            regist_btn:function (req) {
                $('#register').on('click',function () {
                    var r = getQueryString("r");
                    var mobile = $('#mobile').val();
                    var pwd = $('#pwd').val();
                    var cms = $('#cms').val();
                    //手机号
                    if(mobileReg.test(mobile)===false){
                        layer.msg('请输入正确的手机号',{time:3000});
                        return
                    }
                    //密码
                    if(pwdReg.test(pwd) === false){
                        $('.register_tips').show();
                        layer.msg('请输入6-20位密码',{time:3000});
                        return
                    }
                    //验证码
                    if(code.test(cms) === false){
                        $('.register_tips').show();
                        layer.msg('请输入正确的验证码',{time:3000});
                        return
                    }
                    //协议勾选
                    if(flag==false){
                        layer.msg('请勾选协议',{time:3000});
                        return
                    }else{
                        $('.register_tips').hide();
                    }
                    $.post('/other/vl_check_login',{mobile:mobile,code:cms,pwd:pwd,invite:invite},function (res) {
                        if(res.code == -1){
                            layer.msg(res.message,{time:3000});
                            return
                        }else{
                            window.location.href="/";
                        }
                    });
                })
            },
        }
        all.inner_check();
        all.regist_btn();
        all.registerCode();
        $('#download').on('click',function () {
            all.download();
        })
        if(mobile){
           $('#gift_tips').show().text("你的好友"+mobile.replace(/^(\d{3})(.+)(\w{4})$/, "$1****$3")+"送您1188现金大礼");
           $('.register_form>div:nth-of-type(3)').css({'marginTop':'.64rem'})
        }else {
            $('#gift_tips').hide();
            $('.register_form>div:nth-of-type(2)').css({'marginTop':'.64rem'})
        }
    });


</script>
